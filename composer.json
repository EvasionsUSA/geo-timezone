{
    "name": "tochka-developers/geo-timezone",
    "description": "PHP library for timezone calculation from a particular location.",
    "license": "MIT",
    "require": {
        "php": "8.2.*|8.3.*",
        "ext-dom": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "ext-xml": "*",
        "tochka-developers/geophp": "^0.2",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/pint": "^1.14",
        "vimeo/psalm": "^5.23"
    },
    "autoload": {
        "psr-4": {
            "Tochka\\GeoTimeZone\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "psalm": "vendor/bin/psalm --output-format=phpstorm --no-cache"
    }
}
